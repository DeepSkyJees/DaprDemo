{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    }
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "Swagger",
    "title": "Backend Api",
    "version": "v1",
    "routePrefix": "Backend",
    "includeSecurity": false,
    "xmlFile": "Nigel.Dapr.BackendApi"
  }

}
